						<Biome name='.*'  />
						<OreBlock block='gregtech:gt.blockores' weight='0.75'
                    nbt='{m:535, n:1, id:"GT_TileEntity_Ores"}'/><!--Coal-->
						<OreBlock block='gregtech:gt.blockores' weight='0.25'
                    nbt='{m:538, n:1, id:"GT_TileEntity_Ores"}'/><!--Lignite-->
						<Replaces block='minecraft:stone'/>
						<Replaces block='minecraft:dirt'/>
						<Replaces block='minecraft:gravel'/>
						<Replaces block='minecraft:air'/>
						<Setting name='MotherlodeRangeLimit' avg=':= 16 ' range=':= 16 ' type='normal' />
						<Setting name='MotherlodeSize' avg=':= 3 * _default_' range=':= 2.5' type='normal' />
						<Setting name='MotherlodeHeight' avg=':= 90' range=':= 40' type='normal'/>
						<Setting name='BranchFrequency' avg=':= 6 ' range=':= 2 ' type='normal' />
						<Setting name='BranchLength' avg=':= 3 * _default_ ' range=':= 3 * _default_ ' type='normal' />
						<Setting name='BranchHeightLimit' avg='6'/> 
						<Setting name='SegmentRadius' avg=':= 3 * _default_' range=':= 3 * _default_'/>
				</Veins>
			</IfCondition>
		</ConfigSection>
